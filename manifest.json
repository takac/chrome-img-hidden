{
  "manifest_version": 2,

  "name": "One-no img Kittens",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",

  
  "permissions": [
        "background",
        "storage",
        "tabs", 
        "http://*/*",
        "https://*/*"
  ],

  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery.js", "content.js"]
    }
  ]
}
